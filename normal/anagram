def anagram(s,t):
    if(len(s)==len(t)):
        a=list(s)
        b=list(t)
        for x in a:
            if(x not in b):
                print("False")
                break
        else:
             print("True")
    else:
        print("False")
s=input("Enter :")
t=input("Enter:")
anagram(s,t)
